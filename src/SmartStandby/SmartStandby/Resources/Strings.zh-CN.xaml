<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="using:System">

    <system:String x:Key="AppTitle">智能待机 (Smart Standby)</system:String>
    
    <!-- Dashboard -->
    <system:String x:Key="NavDashboard">仪表盘</system:String>
    <system:String x:Key="NavSettings">设置</system:String>
    <system:String x:Key="ButtonSmartSleep">智能睡眠</system:String>
    <system:String x:Key="StatusHealthy">良好</system:String>
    <system:String x:Key="StatusWarning">警告</system:String>
    <system:String x:Key="WakeHealthHeader">唤醒健康度</system:String>
    <system:String x:Key="RecentSessionsHeader">最近会话</system:String>
    <system:String x:Key="NoSessionsMessage">暂无睡眠会话记录。</system:String>

    <!-- Settings -->
    <system:String x:Key="SettingsHeader">应用程序设置</system:String>
    <system:String x:Key="ConfigGeneral">常规配置</system:String>
    <system:String x:Key="ConfigNetwork">睡眠时断开网络连接</system:String>
    <system:String x:Key="ConfigStartup">开机自动启动</system:String>
    <system:String x:Key="ConfigTdr">启用 TDR 修复 (防黑屏)</system:String>
    <system:String x:Key="ConfigThreshold">电池耗电健康阈值 (%/小时)</system:String>
    <system:String x:Key="WhitelistHeader">进程白名单 (安全应用)</system:String>
    <system:String x:Key="ButtonClearHistory">清空历史记录</system:String>
    <system:String x:Key="ButtonOpenLogs">打开日志文件夹</system:String>
    <system:String x:Key="AboutHeader">关于与更新</system:String>
</ResourceDictionary>
